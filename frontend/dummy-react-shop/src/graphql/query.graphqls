query findAllProductCategories{
    findAllProductCategories{
        id
        name
    }
}

query findAllProducts{
    findAllProducts{
        id
        name
        price
        discountedPrice
        discount {
            id
            name
            percentage
        }
        productCategory{
            id
            name
        }
        productInventory{
            id
            quantity
        }
        createdAt
    }
}